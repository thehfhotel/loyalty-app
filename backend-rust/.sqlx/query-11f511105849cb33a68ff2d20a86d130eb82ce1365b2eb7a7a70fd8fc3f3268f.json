{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                u.id,\n                u.email,\n                u.role as \"role: UserRole\",\n                u.is_active,\n                u.email_verified,\n                u.created_at,\n                u.updated_at,\n                u.oauth_provider,\n                up.first_name,\n                up.last_name,\n                up.phone,\n                up.date_of_birth,\n                up.preferences,\n                up.avatar_url,\n                up.membership_id as \"membership_id?\",\n                up.created_at AS profile_created_at,\n                up.updated_at AS profile_updated_at\n            FROM users u\n            LEFT JOIN user_profiles up ON u.id = up.user_id\n            WHERE u.id = $1\n            ",
