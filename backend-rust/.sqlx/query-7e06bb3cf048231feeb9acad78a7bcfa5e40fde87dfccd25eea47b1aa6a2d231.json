  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                u.id,\n                u.email,\n                u.password_hash,\n                u.role as \"role: UserRole\",\n                u.is_active,\n                u.email_verified,\n                u.created_at,\n                u.updated_at,\n                u.oauth_provider,\n                u.oauth_provider_id\n            FROM users u\n            INNER JOIN user_profiles up ON u.id = up.user_id\n            WHERE UPPER(up.membership_id) = UPPER($1)\n            ",
